<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: #3F63DB;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" layoutX="16.0" layoutY="22.0" prefHeight="104.0" prefWidth="461.0" text="Quieres ser mi novia?" textFill="#e4e4e4">
         <font>
            <Font name="Book Antiqua Bold" size="28.0" />
         </font>
      </Label>
      <Button fx:id="yesButton" layoutX="69.0" layoutY="201.0" mnemonicParsing="false" onAction="#yesButton" prefHeight="56.0" prefWidth="119.0" style="-fx-background-color: #C13BEC;" text="Si" textFill="WHITE" />
      <Button fx:id="NoButton" layoutX="306.0" layoutY="201.0" mnemonicParsing="false" onMouseEntered="#NoButton" prefHeight="56.0" prefWidth="119.0" style="-fx-background-color: #C13BEC;" text="No" textFill="WHITE" />
   </children>
</AnchorPane>
